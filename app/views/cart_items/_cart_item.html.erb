<div id="<%= dom_id cart_item %>">
  <p>
    <strong>Product:</strong>
    <%= cart_item.product_id %>
  </p>

  <p>
    <strong>Cart:</strong>
    <%= cart_item.cart_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= cart_item.quantity %>
  </p>

  <p>
    <strong>Line total:</strong>
    <%= cart_item.line_total %>
  </p>

</div>
